#include <iostream>

using namespace std;

int main()
{
   int x1=0,y1=0,x2=6,y2=2;
   int x3=5,y3=-4,x4=2,y4=2;
   cin>>x1>>y1>>x2>>y2>>x3>>y3>>x4>>y4;
    
   double f1 = (x2 - x1) * (y3 - y1) - (y2 - y1) * (x3 - x1);
	double f2 = (x2 - x1) * (y4 - y1) - (y2 - y1) * (x4 - x1);
   if(f1*f2<0)
       cout<<1<<endl;
   else
       cout<<0<<endl;
    
    return 0;
