#include <iostream>
#include <tuple>
#include <vector>
#include <algorithm>
#define endl "\n"
using namespace std;
vector<tuple<int,string,int>> bojMem;
int main()
{
   int bojMems =3;
    cin>>bojMems;
    
    for(int i=0; i<bojMems; i++)
    {
        int age,idx =0;
        string name="";
        cin>>age>>name;
        bojMem.push_back(tuple<int,string,int>(age,name,i));
    }

   sort(bojMem.begin(),bojMem.end(),[&](tuple<int,string,int> a, tuple<int,string,int> b)
   {
      if(get<0>(a) <= get<0>(b))
      {
          if(get<0>(a) == get<0>(b))
          {
              return get<2>(a)<get<2>(b);
          }
          else
              return true;
      }
      else
          return false;
   });
   
   for(int i=0; i<bojMem.size(); i++)
   {
       cout<<get<0>(bojMem[i])<<" "<<get<1>(bojMem[i])<<endl;
   }
   cout<<endl;
}
